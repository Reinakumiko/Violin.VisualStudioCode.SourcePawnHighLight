{
	"Include": {
		"prefix": "include",
		"description": "Include a new library",
		"body": [
			"#include <${include}>"
		]
	},
	"Define":{
		"prefix": "define",
		"description": "A new define",
		"body": [
			"#define ${Name} ${body}"
		]
	},
	"Class": {
		"prefix": "methodmap",
		"body": [
			"methodmap ${Name}",
			"{",
			"	$0",
			"}"
		],
		"description": "Decalre a new Class(methodmap)"
	}
}